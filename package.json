{
  "name": "howlonguntildspgetsajob",
  "version": "1.0.0",
  "description": "Website for howlonguntildspgetsajob.com",
  "main": "dist/index.html",
  "author": "Alexandros Katechis",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "copysnd": "mkdir dist && cp -R ./src/sounds ./dist/sounds",
    "build": "yarn clean && yarn copysnd && parcel build src/index.html",
    "dev": "yarn clean && yarn copysnd && parcel src/index.html --port 8080"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "date-fns": "^2.8.1",
    "milligram": "^1.3.0",
    "parcel-bundler": "^1.12.4"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  }
}
